    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@genql/cli": "^2.6.0",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "nodemon": "^2.0.14",
    "prisma": "^3.3.0",
    "ts-node": "^10.4.0",
